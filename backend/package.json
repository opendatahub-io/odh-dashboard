{
  "name": "odh-dashboard-backend",
  "version": "2.0.0",
  "description": "Backend for the ODH dashboard.",
  "license": "Apache-2.0",
  "private": true,
  "packageManager": "npm@10.9.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendatahub-io/odh-dashboard.git"
  },
  "homepage": "https://github.com/opendatahub-io/odh-dashboard#readme",
  "bugs": {
    "url": "https://github.com/opendatahub-io/odh-dashboard/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "src/server.ts",
  "scripts": {
    "clean": "rm -f ../logs/adminActivity.log",
    "start": "NODE_ENV=production node ./dist/server.js",
    "start:dev": "npm run clean && NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development nodemon src/server.ts",
    "debug": "NODE_TLS_REJECT_UNAUTHORIZED=0 NODE_ENV=development nodemon --exec 'node --inspect --require ts-node/register' src/server.ts",
    "start:dev:wait": "npx wait-on -i 1000 http://localhost:4000",
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "test": "run-s test:lint test:type-check test:jest",
    "test:unit-coverage": "npm run test:jest:coverage",
    "test:jest:coverage": "rimraf jest-coverage && jest --silent --coverage --coverageReporters=json --coverageReporters=lcov",
    "test:lint": "eslint --max-warnings 0 --ext .json,.js,.ts ./src",
    "test:fix": "eslint --ext .json,.js,.ts ./src --fix",
    "test:type-check": "turbo run type-check",
    "test:jest": "jest",
    "type-check": "tsc --noEmit",
    "server": "NODE_ENV=production node ./dist/server.js",
    "lint": "eslint .",
    "test-unit": "jest --silent",
    "test-unit-coverage": "jest --silent --coverage"
  },
  "dependencies": {
    "@fastify/accepts": "^4.3.0",
    "@fastify/autoload": "^5.7.1",
    "@fastify/http-proxy": "^9.5.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.10.2",
    "@fastify/view": "^9.1.0",
    "@fastify/websocket": "^8.3.1",
    "@kubernetes/client-node": "^0.12.2",
    "@odh-dashboard/app-config": "*",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "fastify": "^4.28.1",
    "fastify-plugin": "^4.5.1",
    "http-errors": "^1.8.0",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "pino": "^9.9.0",
    "prom-client": "^14.0.1",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@odh-dashboard/eslint-config": "*",
    "@types/http-errors": "^1.8.0",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.14.5",
    "globals": "^16.4.0",
    "nodemon": "^3.0.2",
    "pino-pretty": "^9.4.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsdown": "^0.15.6",
    "typescript": "^5.8.3"
  },
  "optionalDependencies": {
    "@types/jest": "^29.5.3",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1"
  },
  "overrides": {
    "tough-cookie": "^4.1.3",
    "ws": "^8.17.1",
    "@types/tar": "^6.1.13",
    "jsonpath-plus": "^10.3.0"
  }
}
