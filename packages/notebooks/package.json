{
  "private": true,
  "name": "@odh-dashboard/notebooks",
  "description": "Notebooks plugin.",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "update-subtree": "package-subtree --package=@odh-dashboard/notebooks",
    "install:module": "npm install --prefix upstream",
    "start:dev": "cd upstream && PORT=9105 DEPLOYMENT_MODE=federated npm run start:dev",
    "start:dev:ext": "cd upstream && DEPLOYMENT_MODE=federated PORT=9105 npm run start:dev",
    "start:dev:wait": "npx wait-on -i 1000 http://localhost:9105"
  },
  "subtree": {
    "repo": "https://github.com/kubeflow/notebooks.git",
    "src": "workspaces/frontend",
    "target": "upstream",
    "commit": "68d9466e337e50e604209b1a16c39ddc2cce91d7"
  },
  "module-federation": {
    "name": "notebooks",
    "remoteEntry": "/remoteEntry.js",
    "authorize": true,
    "tls": false,
    "proxy": [
      {
        "path": "/workspaces/api",
        "pathRewrite": "/api"
      }
    ],
    "local": {
      "host": "localhost",
      "port": 9105
    },
    "service": {
      "name": "odh-dashboard",
      "port": 8043
    }
  },
  "dependencies": {
    "@odh-dashboard/internal": "*"
  },
  "devDependencies": {
    "@odh-dashboard/eslint-config": "*",
    "@odh-dashboard/tsconfig": "*"
  }
}
