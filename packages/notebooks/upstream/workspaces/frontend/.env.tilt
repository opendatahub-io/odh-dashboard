APP_ENV=development

DEPLOYMENT_MODE=federated
MOCK_API_ENABLED=false
MANDATORY_NAMESPACE=default
ROUTES_PREFIX=/notebooks
URL_PREFIX=/workspaces
PUBLIC_PATH=/

# Server configuration - use port 8080 to match deployment manifest
PORT=8080

# Proxy configuration for Tilt development
# Frontend runs in Kubernetes and reaches backend via service name
PROXY_HOST=workspaces-backend.kubeflow-workspaces.svc.cluster.local
PROXY_PORT=4000
PROXY_PROTOCOL=http
