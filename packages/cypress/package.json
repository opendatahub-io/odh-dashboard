{
  "name": "@odh-dashboard/cypress",
  "version": "0.0.0",
  "private": true,
  "description": "Cypress tests for the ODH Dashboard.",
  "scripts": {
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "open:mock": "CY_MOCK=1 CY_WS_PORT=9002 cypress open",
    "open:e2e": "cypress open --env grepTags=\"${CY_TEST_TAGS}\",skipTags=\"${CY_SKIP_TAGS}\"",
    "record:open": "CY_RECORD=1 cypress open",
    "record:run": "CY_RECORD=1 cypress run -b chrome && npm run record:format",
    "record:format": "prettier --write cypress/**/*.snap.json",
    "run:mock": "CY_MOCK=1 CY_WS_PORT=9002 cypress run -b chrome",
    "run:mock:coverage": "rimraf coverage .nyc_output && CY_COVERAGE=true npm run run:mock",
    "run:e2e": "cypress run --env grepTags=\"${CY_TEST_TAGS}\",skipTags=\"${CY_SKIP_TAGS}\" -b chrome",
    "run:e2e:dashboard": "cypress run --env grepTags=@Dashboard+-@Bug -b chrome",
    "run:electron": "cypress run -b electron"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.34",
    "@cypress/grep": "^4.1.0",
    "@odh-dashboard/app-config": "*",
    "@odh-dashboard/eslint-config": "*",
    "@odh-dashboard/feature-store": "*",
    "@odh-dashboard/internal": "*",
    "@odh-dashboard/llmd-serving": "*",
    "@odh-dashboard/model-registry": "*",
    "@odh-dashboard/model-serving": "*",
    "@odh-dashboard/model-training": "*",
    "@openshift/dynamic-plugin-sdk-utils": "^5.0.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.4",
    "@types/chai-subset": "^1.3.5",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/ws": "^8.5.10",
    "chai-subset": "^1.6.0",
    "cypress": "^13.14.2",
    "cypress-axe": "^1.5.0",
    "cypress-high-resolution": "^1.0.0",
    "cypress-mochawesome-reporter": "3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-plugin-steps": "^1.1.1",
    "dotenv": "^16.4.7",
    "https-proxy-agent": "^7.0.6",
    "istanbul-merge": "^2.0.0",
    "js-yaml": "^4.0.0",
    "lodash-es": "^4.17.15",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ws": "^8.17.1"
  }
}
