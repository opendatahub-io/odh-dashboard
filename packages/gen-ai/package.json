{
  "private": true,
  "name": "@odh-dashboard/gen-ai",
  "description": "Gen AI plugin.",
  "version": "0.0.0",
  "module-federation": {
    "name": "genAi",
    "remoteEntry": "/remoteEntry.js",
    "authorize": true,
    "tls": false,
    "proxy": [
      {
        "path": "/gen-ai/api",
        "pathRewrite": "/api"
      }
    ],
    "local": {
      "host": "localhost",
      "port": 9102
    },
    "service": {
      "name": "odh-dashboard",
      "port": 8043
    }
  },
  "dependencies": {
    "@odh-dashboard/plugin-core": "*"
  },
  "devDependencies": {
    "@odh-dashboard/eslint-config": "*",
    "@odh-dashboard/tsconfig": "*"
  }
}
