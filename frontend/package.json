{
  "name": "odh-dashboard-frontend",
  "version": "0.1.0",
  "description": "Frontend for ODH dashboard.",
  "author": "",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendatahub-io/odh-dashboard.git"
  },
  "homepage": "https://github.com/opendatahub-io/odh-dashboard#readme",
  "bugs": {
    "url": "https://github.com/opendatahub-io/odh-dashboard/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "run-s build:prod",
    "build:analyze": "export _ODH_OUTPUT_ONLY=true; run-s build build:bundle-profile build:bundle-analyze",
    "build:bundle-profile": "webpack --config ./config/webpack.prod.js --profile --json > ./bundle.stats.json",
    "build:bundle-analyze": "webpack-bundle-analyzer ./bundle.stats.json",
    "build:clean": "rimraf ./public",
    "build:prod": "webpack --config ./config/webpack.prod.js",
    "format": "prettier --write \"./**/*.ts\" \"./**/*.tsx\" \"./**/*.scss\"",
    "start:dev": "webpack serve --hot --color --config ./config/webpack.dev.js",
    "test": "run-s test:lint test:jest test:type-check",
    "test:jest": "jest",
    "test:ci": "export CI=true; jest --collectCoverage",
    "test:clearCache": "jest --clearCache",
    "test:dev": "run-s test:lint test:local",
    "test:fix": "eslint --ext .json,.js,.ts,.jsx,.tsx ./src --fix",
    "test:lint": "eslint --max-warnings 0 --ext .json,.js,.ts,.jsx,.tsx ./src",
    "test:local": "jest --watchAll",
    "test:type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@openshift/dynamic-plugin-sdk": "^1.0.0-alpha12",
    "@openshift/dynamic-plugin-sdk-utils": "^1.0.0-alpha17",
    "@patternfly/patternfly": "4.222.4",
    "@patternfly/quickstarts": "^2.3.1",
    "@patternfly/react-catalog-view-extension": "4.93.15",
    "@patternfly/react-core": "4.267.6",
    "@patternfly/react-icons": "4.93.3",
    "@patternfly/react-styles": "4.92.3",
    "@patternfly/react-table": "4.112.6",
    "@patternfly/react-virtualized-extension": "4.88.82",
    "@types/classnames": "^2.3.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "compare-versions": "^3.6.0",
    "dompurify": "^2.2.6",
    "lodash-es": "^4.17.15",
    "react": "^18.2.0",
    "react-cool-dimensions": "^2.0.5",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.4",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.56.2",
    "showdown": "^1.9.1",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^26.0.14",
    "@types/node": "^17.0.29",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/showdown": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^5.5.0",
    "imagemin": "^7.0.0",
    "jest": "^26.6.0",
    "js-yaml-loader": "^1.2.2",
    "mini-css-extract-plugin": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
