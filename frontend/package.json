{
  "name": "odh-dashboard-frontend",
  "version": "0.1.0",
  "description": "Frontend for ODH dashboard.",
  "author": "",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendatahub-io/odh-dashboard.git"
  },
  "homepage": "https://github.com/opendatahub-io/odh-dashboard#readme",
  "bugs": {
    "url": "https://github.com/opendatahub-io/odh-dashboard/issues"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "run-s build:prod",
    "build:for-deploy": "react-scripts build",
    "build:analyze": "export _ODH_OUTPUT_ONLY=true; run-s build build:bundle-profile build:bundle-analyze",
    "build:bundle-profile": "webpack --config ./config/webpack.prod.js --profile --json > ./bundle.stats.json",
    "build:bundle-analyze": "webpack-bundle-analyzer ./bundle.stats.json",
    "build:clean": "rimraf ./public",
    "build:prod": "webpack --config ./config/webpack.prod.js",
    "format": "prettier --write \"./**/*.ts\" \"./**/*.tsx\" \"./**/*.scss\"",
    "start:dev": "webpack serve --hot --color --config ./config/webpack.dev.js",
    "test": "run-s test:lint test:jest test:type-check",
    "test:jest": "jest",
    "test:ci": "export CI=true; jest --collectCoverage",
    "test:clearCache": "jest --clearCache",
    "test:dev": "run-s test:lint test:local",
    "test:fix": "eslint --ext .json,.js,.ts,.jsx,.tsx ./src --fix",
    "test:lint": "eslint --max-warnings 0 --ext .json,.js,.ts,.jsx,.tsx ./src",
    "test:local": "jest --watchAll",
    "test:type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@patternfly/patternfly": "4.215.1",
    "@patternfly/quickstarts": "^2.3.1",
    "@patternfly/react-catalog-view-extension": "4.90.0",
    "@patternfly/react-core": "4.239.0",
    "@patternfly/react-icons": "4.90.0",
    "@patternfly/react-styles": "4.89.0",
    "@patternfly/react-table": "4.108.0",
    "@types/classnames": "^2.3.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "compare-versions": "^3.6.0",
    "dompurify": "^2.2.6",
    "lodash-es": "^4.17.15",
    "node-gyp": "^7.1.2",
    "node-sass": "^4.14.1",
    "react": "^17.0.2",
    "react-cool-dimensions": "^2.0.5",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.1",
    "react-router": "^6.4.1",
    "react-router-dom": "^6.4.1",
    "react-scripts": "^4.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "showdown": "^1.9.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^26.0.14",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.3.3",
    "@types/showdown": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "imagemin": "^7.0.0",
    "jest": "^26.6.0",
    "js-yaml-loader": "^1.2.2",
    "mini-css-extract-plugin": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "redux-mock-store": "^1.5.4",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.3.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^26.4.1",
    "ts-loader": "^8.0.5",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^5.2.0"
  }
}
