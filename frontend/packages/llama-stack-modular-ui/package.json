{
  "private": true,
  "name": "@odh-dashboard/llama-stack-modular-ui",
  "description": "LLama Stack Modular UI plugin.",
  "version": "0.0.0",
  "module-federation": {
    "name": "llamaStackModularUi",
    "remoteEntry": "/remoteEntry.js",
    "authorize": true,
    "tls": false,
    "proxy": [
      {
        "path": "/llama-stack/genai",
        "pathRewrite": "/genai"
      }
    ],
    "local": {
      "host": "localhost",
      "port": 9002
    },
    "service": {
      "name": "odh-dashboard",
      "port": 8043
    }
  },
  "dependencies": {
    "@odh-dashboard/plugin-core": "*"
  },
  "devDependencies": {
    "@odh-dashboard/eslint-config": "*",
    "@odh-dashboard/tsconfig": "*"
  }
}
