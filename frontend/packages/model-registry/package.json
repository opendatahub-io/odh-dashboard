{
  "private": true,
  "name": "@odh-dashboard/model-registry",
  "description": "Model registry plugin.",
  "version": "0.0.0",
  "scripts": {
    "update-subtree": "../../scripts/package-subtree.sh model-registry",
    "test:contract": "jest --config=pact/jest.contract.config.js",
    "test:contract:watch": "npm run test:contract -- --watch",
    "test:contract:with-bff": "pact/scripts/run-with-mock-bff.sh"
  },
  "subtree": {
    "repo": "https://github.com/kubeflow/model-registry.git",
    "src": "clients/ui",
    "target": "upstream",
    "commit": "b4c43a0dfc6864f9a630a011e71f48c48b695df1"
  },
  "module-federation": {
    "name": "modelRegistry",
    "remoteEntry": "/remoteEntry.js",
    "authorize": true,
    "tls": false,
    "proxy": [
      {
        "path": "/model-registry/api",
        "pathRewrite": "/api"
      }
    ],
    "local": {
      "host": "localhost",
      "port": 9000
    },
    "service": {
      "name": "odh-dashboard",
      "port": 8043
    }
  },
  "exports": {
    "./extensions": "./extensions.ts"
  },
  "dependencies": {
    "@odh-dashboard/internal": "*",
    "@odh-dashboard/pact-testing": "*"
  },
  "devDependencies": {
    "@odh-dashboard/eslint-config": "*",
    "@odh-dashboard/tsconfig": "*"
  },
  "optionalDependencies": {
    "@types/jest": "^29.5.13",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-html-reporters": "^3.1.7",
    "axios": "^1.6.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  }
}