{
  "private": true,
  "name": "@odh-dashboard/model-registry",
  "description": "Model registry plugin.",
  "version": "0.0.0",
  "scripts": {
    "update-subtree": "../../scripts/package-subtree.sh model-registry"
  },
  "subtree": {
    "repo": "https://github.com/lucferbux/model-registry.git",
    "src": "clients/ui",
    "target": "upstream",
    "commit": "1783e5e021640b98e6881f3ff442852845007d0b"
  },
  "module-federation": {
    "name": "modelRegistry",
    "remoteEntry": "/remoteEntry.js",
    "authorize": true,
    "tls": false,
    "proxy": [
      {
        "path": "/model-registry/api",
        "pathRewrite": "/api"
      }
    ],
    "local": {
      "host": "localhost",
      "port": 9000
    },
    "service": {
      "name": "model-registry-ui-service",
      "port": 8080
    }
  }
}
